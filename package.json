{
  "name": "sveltekit-omakase",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:push:remote": "NODE_ENV=production drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:remote": "NODE_ENV=production drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "db:studio:remote": "NODE_ENV=production drizzle-kit studio",
    "test": "pnpm test:e2e && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:e2e": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.14.1",
    "bits-ui": "1.3.19",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "formsnap": "2.0.1",
    "globals": "^16.0.0",
    "mode-watcher": "^1.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "svelte-sonner": "^0.3.28",
    "sveltekit-rate-limiter": "^0.6.1",
    "sveltekit-superforms": "^2.24.1",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-node": "^3.1.2",
    "vitest": "^3.1.2",
    "zod": "^3.24.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@libsql/client": "^0.15.4",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pilcrowjs/object-parser": "^0.0.4",
    "arctic": "^3.6.0",
    "drizzle-orm": "^0.42.0",
    "lucide-svelte": "^0.503.0",
    "mjml": "^4.15.3",
    "nanoid": "^5.1.5",
    "resend": "^4.4.0",
    "sailkit": "^0.2.3",
    "svelte-radix": "^2.0.1",
    "sveltekit-flash-message": "^2.4.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
