{
  "type": "module",
  "name": "sveltekit-omakase",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:push:remote": "NODE_ENV=production drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:remote": "NODE_ENV=production drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "db:studio:remote": "NODE_ENV=production drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.51.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.19.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.0.14",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.10",
    "bits-ui": "1.3.12",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.2.1",
    "formsnap": "2.0.0",
    "globals": "^16.0.0",
    "mode-watcher": "^0.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "svelte-sonner": "^0.3.28",
    "sveltekit-rate-limiter": "^0.6.1",
    "sveltekit-superforms": "^2.24.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@libsql/client": "^0.14.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pilcrowjs/object-parser": "^0.0.4",
    "@sendgrid/mail": "^8.1.4",
    "arctic": "^3.5.0",
    "drizzle-orm": "^0.40.0",
    "lucide-svelte": "^0.482.0",
    "mjml": "^4.15.3",
    "nanoid": "^5.1.4",
    "sailkit": "^0.2.2",
    "svelte-radix": "^2.0.1",
    "sveltekit-flash-message": "^2.4.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
