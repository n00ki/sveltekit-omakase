{
  "name": "sveltekit-omakase",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "postinstall": "npx simple-git-hooks",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:fix": "prettier --write . && eslint . --fix",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "test": "playwright test && vitest",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@internationalized/date": "^3.11.0",
    "@lucide/svelte": "^0.564.0",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-auto": "^7.0.1",
    "@sveltejs/kit": "^2.51.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.2.3",
    "@types/uuid": "^11.0.0",
    "bits-ui": "2.15.5",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "lint-staged": "^16.2.7",
    "mode-watcher": "^1.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "simple-git-hooks": "^2.13.1",
    "svelte": "5.51.0",
    "svelte-check": "^4.4.0",
    "svelte-sonner": "^1.0.7",
    "sveltekit-flash-message": "^2.4.6",
    "sveltekit-rate-limiter": "^0.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-node": "^5.3.0",
    "vitest": "^4.0.18",
    "zod": "4.3.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@libsql/client": "^0.17.0",
    "better-auth": "^1.4.18",
    "drizzle-orm": "^0.45.1",
    "mjml": "^4.18.0",
    "nanoid": "^5.1.6",
    "resend": "^6.9.2",
    "sailkit": "^0.3.5",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,svelte,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
