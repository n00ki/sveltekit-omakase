{
  "name": "sveltekit-omakase",
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/seed",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "test": "pnpm test:e2e && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write --plugin prettier-plugin-svelte .",
    "test:e2e": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@lucide/svelte": "^0.513.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/table-core": "^8.21.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.30",
    "bits-ui": "2.4.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.1",
    "formsnap": "2.0.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "layerchart": "2.0.0-next.6",
    "lint-staged": "^16.1.0",
    "mode-watcher": "^1.0.7",
    "paneforge": "1.0.0-next.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "svelte": "5.33.17",
    "svelte-check": "^4.2.1",
    "svelte-sonner": "^1.0.4",
    "sveltekit-flash-message": "^2.4.6",
    "sveltekit-rate-limiter": "^0.6.1",
    "sveltekit-superforms": "^2.26.1",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^6.3.5",
    "vite-node": "^3.2.2",
    "vitest": "^3.2.2",
    "zod": "^3.25.56"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@libsql/client": "^0.15.8",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@pilcrowjs/object-parser": "^0.0.4",
    "arctic": "^3.7.0",
    "drizzle-orm": "^0.44.2",
    "mjml": "^4.15.3",
    "nanoid": "^5.1.5",
    "resend": "^4.5.2",
    "sailkit": "^0.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "lint-staged": {
    "*": "pnpm lint"
  }
}
