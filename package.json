{
  "type": "module",
  "name": "sveltekit-omakase",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:push:remote": "NODE_ENV=production drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:remote": "NODE_ENV=production drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:clear": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/clear",
    "db:studio": "drizzle-kit studio",
    "db:studio:remote": "NODE_ENV=production drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:clear && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.1",
    "autoprefixer": "^10.4.20",
    "bits-ui": "1.0.0-next.52",
    "clsx": "^2.1.1",
    "cssnano": "^7.0.6",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "formsnap": "2.0.0-next.1",
    "globals": "^15.13.0",
    "mode-watcher": "^0.5.0",
    "postcss": "^8.4.49",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.4.1",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.4.0",
    "svelte-check": "^4.1.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-rate-limiter": "^0.6.1",
    "sveltekit-superforms": "^2.21.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.2",
    "vite-node": "^2.1.8",
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.703.0",
    "@aws-sdk/s3-request-presigner": "^3.703.0",
    "@libsql/client": "^0.14.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sendgrid/mail": "^8.1.4",
    "drizzle-orm": "^0.36.4",
    "nanoid": "^5.0.9",
    "oslo": "^1.2.1",
    "svelte-radix": "^2.0.1",
    "sveltekit-flash-message": "^2.4.4"
  }
}
